{
  "author": "code@virtualpatterns.com",
  "description": "Virtualize your Pug templates",
  "license": "ISC",
  "name": "nessa",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/nessa"
  },
  "scripts": {
    "link": "hln ./process/logs \"$HOME/Library/Logs/nessa\"",
    "unlink": "hln -u \"$HOME/Library/Logs/nessa\"",
    "clean": "rm -rf ./coverage ./library ./server ./tests ./www",
    "build": "npm run clean && babel ./source --copy-files --out-dir . && webpack",
    "test-library": "rm -f process/logs/nessa.mocha.log && npm run build && istanbul cover ./node_modules/.bin/_mocha --dir ./coverage -- --bail --timeout 0 ./tests",
    "debug-test-library": "npm run build && node-debug ./node_modules/.bin/_mocha --bail --timeout 0 ./tests",
    "test-www": "npm run build && mocha-phantomjs --bail --hooks ./resources/hooks.js --ignore-resource-errors --path ./node_modules/.bin/phantomjs http://localhost:8082/www/test.html",
    "run": "npm run build && node server/index.js"
  },
  "version": "0.0.1",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.22.0",
    "istanbul": "^0.4.5",
    "jstransformer-babel": "^2.1.0",
    "jstransformer-cdata-js": "^1.0.0",
    "jstransformer-markdown-it": "^2.0.0",
    "mocha": "^3.2.0",
    "mocha-phantomjs": "^4.1.0",
    "phantomjs-prebuilt": "^2.1.14",
    "restify": "^4.3.0",
    "vdom-to-html": "https://github.com/virtualpatterns/vdom-to-html",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "@pwn/is": "^1.0.2",
    "babel-core": "^6.22.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015-nostrict": "^6.6.2",
    "detect-node": "^2.0.3",
    "es6-promisify": "^5.0.0",
    "escape-html": "^1.0.3",
    "foreach": "^2.0.5",
    "js-beautify": "^1.6.8",
    "jsesc": "^2.4.0",
    "loader-utils": "^0.2.16",
    "mkdirp": "^0.5.1",
    "pad": "^1.0.2",
    "prettier": "^0.11.0",
    "pug": "^2.0.0-beta6",
    "pug-filters": "^2.1.0",
    "pug-lexer": "^2.3.0",
    "pug-loader": "^2.3.0",
    "pug-parser": "^2.0.1",
    "resolve-path": "^1.3.3",
    "to-js-identifier": "^1.0.0",
    "touch": "^1.0.0",
    "virtual-dom": "^2.1.1",
    "winston": "^2.3.0",
    "with": "^5.1.1"
  }
}
